    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "jest": {
    "setupFiles": [
      "./tests/setup.js"
    ]
  },
  "dependencies": {
    "authenticare": "^0.3.0",
    "dotenv": "^7.0.0",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.17.3",
    "formidable": "^1.2.1",
    "knex": "^0.16.3",
    "nodemon": "^1.19.0",
    "react-bulma-components": "^2.3.0",
    "react-google-maps": "^9.4.5",
    "react-html5-camera-photo": "^1.4.1",
    "react-redux": "^7.0.2",
    "redux": "^4.0.1",
    "sqlite3": "^4.0.6",
    "superagent": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
